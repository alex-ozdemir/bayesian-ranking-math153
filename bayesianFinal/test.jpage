double dx = 0.01;
double dy = 0.01;
double volume = 0;
for (double x = 0.0; x < 10.0; x = x + dx) {
	for (double y = 0.0; y < 10.0 - x; y = y + dy) {
		volume += (10.0 - x - y) * dx * dy;
	}
}
System.out.println("Volume: " + volume);